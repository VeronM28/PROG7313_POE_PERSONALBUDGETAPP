<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/mzansi_money"
        android:label="@string/app_name"
        android:roundIcon="@drawable/mzansi_money"
        android:supportsRtl="true"
        android:theme="@style/Theme.PROG7313_POE_PERSONALBUDGETAPP"
        tools:targetApi="31">
        <activity android:name=".ui.home.MainPageActivity" />

        <activity android:name=".LaunchPageActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>



</manifest>